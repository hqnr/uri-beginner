import java.util.Scanner;

public class uri {

	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		
		int a, b, c, maior;
		
		a = input.nextInt();
		b = input.nextInt();
		c = input.nextInt();
		
		maior = (a + b + Math.abs(a - b))/2;
		maior = (maior + c + Math.abs(maior - c))/2;
		
		System.out.printf("%d eh o maior", maior);
	
	}

}
